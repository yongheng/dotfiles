[user]
	name = Yongheng Lin
	email = yongheng.lin@gmail.com
[core]
	editor = vim
	autocrlf = false
	filemode = false
[merge]
	tool = vimdiff
[color]
	ui = true
[alias]
	s = status
	b = branch
	d = diff
	l = log
	c = log --oneline --graph --decorate
	f = c --name-status
	cm = c master..
	fm = f master..
	cd = c develop..
	fd = f develop..
[push]
	default = current
[credential]
	helper = cache --timeout=7200
